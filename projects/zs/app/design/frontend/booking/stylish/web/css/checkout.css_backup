#co-payment-form{padding-top:10px}
#back-top .sprimg{background:#008389;bottom:45px;cursor:pointer;height:50px;position:fixed;right:2%;width:50px;border-radius:100%;box-shadow:0 5px 10px rgba(0,0,0,0.2);z-index:100}
#back-top .sprimg span{display:none}
#back-top .sprimg:before{content:"";border-left:10px solid transparent;border-right:10px solid transparent;border-bottom:10px solid #fff;position:absolute;display:inline-block;left:15px;top:20px}
#back-top .sprimg:after{content:"";border-left:10px solid transparent;border-right:10px solid transparent;border-bottom:10px solid #008389;position:absolute;display:inline-block;left:15px;top:22px}
.action-auth-toggle:focus{background:#FF595F}
.page-footer{background:none;padding:0}
.sprite{display:inline-block;background:url(../images/sprite.png) no-repeat 0 0}
