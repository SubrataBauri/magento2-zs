.checkout-index-index .control .captcha-image img{float:left;margin-right:20px}
.checkout-index-index .field.captcha{padding-top:0;display:inline-block;width:100%}
.checkout-index-index .control .captcha{padding-top:20px}
.checkout-index-index .form-login .fieldset .remind{text-decoration:none;color:#008389;font-size:1.2rem;margin:14px 0 30px;display:block}
.checkout-index-index .action.primary{background:#FF595F;border:1px solid #FF595F}
.checkout-index-index .checkout-payment-method .billing-address-details{padding:0;font-size:1.2rem;color:#666}
.opc-block-summary .table-totals tbody .mark,.minicart-items .product-item-name,.opc-block-summary .table-totals tbody .amount{font-size:1.3rem;padding:10px 0;color:#666}
