.checkout-index-index .footer-content{width:100%;border-bottom:1px solid #d2d2d2;border-top:1px solid #d2d2d2;padding:10px 0 0;background:#fff url(../images/footer-bg.png) no-repeat 0 0;background-size:cover}
.checkout-index-index .page-footer .lang-price-select{float:left;margin-right:200px;width:200px}
