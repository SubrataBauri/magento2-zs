.checkout-index-index .lang-price-select ul li.currency-EUR.switcher-option a{text-decoration:none;color:#0C1218;padding:10px 20px;text-transform:capitalize}
.checkout-index-index .lang-price-select ul li.currency-INR.switcher-option a:hover,.checkout-index-index .lang-price-select ul li.currency-EUR.switcher-option a:hover{color:#fff}
