.authentication-wrapper button:hover{background:#008389;text-decoration:none;color:#fff}
.checkout-payment-method .payment-method-content>.actions-toolbar>.primary .action.primary{cursor:pointer;-webkit-transition:all .5s ease;-o-transition:all .5s ease;transition:all .5s ease;display:inline-block;margin:0;background-color:#ff5a60;border-bottom:0 solid #0A568A;color:#fff;padding:5px 15px;border-radius:2px;margin-right:0;font-size:1.2rem}
