<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
<body>
   <referenceContainer name="page.top">
           <container name="topnavigation" as="topnavigation" label="Top Navigation" htmlTag="div" htmlClass="topnavigation">
	          <block class="Apptha\Airhotels\Block\Navigation\Top" name="page.main.topnavigation" template="Magento_Theme::html/navigation/top.phtml"/>
	       </container>
    </referenceContainer>
     <referenceContainer name="sidebar.additional">
           <block class="Apptha\Airhotels\Block\Profile\Profiledashboardleft" name="airhotels_profiledashboard" template="airhotels/profile/profiledashboardleft.phtml" />
     </referenceContainer>
     <referenceContainer name="content">
           <block class="Apptha\Airhotels\Block\Profile\Dashboard" name="airhotels_dashboard" template="airhotels/profile/dashboard.phtml" />
     </referenceContainer>
     </body>
</page>
